{
   "manifest_version": 2,
   "name": "Informatics Course Numbers",
   "description": "Replace all Informatics courses with their old course numbers!",
   "version": "1.0",
   "content_scripts": [
      {
         "matches": [
            "*://*.uw.edu/*",
            "*://*.washington.edu/*"
         ],
         "js": [ "jquery.js", "content.js" ],
         "run_at": "document_end"
      }
   ],
   "background" : {
      "scripts": ["background.js"]
   },
   "browser_action": {
      "default_icon": "logo.png",
      "default_popup": "options.html"
   }
}
